	"item": [
		{
			"name": "Create User",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var randomnum = Math.floor((Math.random() * 1000) + 1);\r",
							"var randomstr = \"space\" + Math.random().toString(36).substring(2);\r",
							"\r",
							"pm.environment.set(\"id\", randomnum);\r",
							"pm.collectionVariables.set(\"username\", randomstr);\r",
							"pm.collectionVariables.set(\"firstName\", randomstr);\r",
							"pm.collectionVariables.set(\"lastName\", randomstr);\r",
							"pm.collectionVariables.set(\"email\", randomstr + \"@gmail.com\");\r",
							"pm.collectionVariables.set(\"password\", randomstr);\r",
							"pm.collectionVariables.set(\"phone\", \"010101010101\");\r",
							"pm.collectionVariables.set(\"userStatus\", 0);\r",
							"\r",
							"console.log(randomnum);\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "accept",
						"value": "application/xml"
					},
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"id\": \"{{id}}\",\r\n  \"username\": \"{{username}}\",\r\n  \"firstName\": \"{{firstName}}\",\r\n  \"lastName\": \"{{lastName}}\",\r\n  \"email\": \"{{email}}\",\r\n  \"password\": \"{{password}}\",\r\n  \"phone\": \"{{phone}}\",\r\n  \"userStatus\": \"{{userStatus}}\"\r\n}\r\n",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "https://petstore.swagger.io/v2/user",
					"protocol": "https",
					"host": [
						"petstore",
						"swagger",
						"io"
					],
					"path": [
						"v2",
						"user"
					]
				},
				"description": "Generated from cURL: curl -X 'POST' \\\r\n  'https://petstore.swagger.io/v2/user' \\\r\n  -H 'accept: application/xml' \\\r\n  -H 'Content-Type: application/json' \\\r\n  -d '{\r\n  \"id\": 0,\r\n  \"username\": \"moon\",\r\n  \"firstName\": \"moon\",\r\n  \"lastName\": \"space\",\r\n  \"email\": \"spacemoon01@gmail.com\",\r\n  \"password\": \"moon01\",\r\n  \"phone\": \"123456\",\r\n  \"userStatus\": 0\r\n}'"
			},
			"response": []
		},
		{
			"name": "Get user by name",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"ID and firstName Matched\",()=> {\r",
							"\r",
							"var jsonData=pm.response.json();\r",
							"pm.expect(jsonData.id).to.eql(pm.environment.get(\"id\"));\r",
							"pm.expect(jsonData.firstName).to.eql(pm.collectionVariables.get(\"firstName\"));\r",
							"\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "https://petstore.swagger.io/v2/user/{{firstName}}",
